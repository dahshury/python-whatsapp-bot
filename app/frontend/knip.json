{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "includeEntryExports": true,
  "entry": [
    "app/**/*.{ts,tsx}",
    "widgets/**/*.{ts,tsx}",
    "features/**/*.{ts,tsx}",
    "shared/**/*.{ts,tsx}",
    "entities/**/*.{ts,tsx}",
    "compositions/**/*.{ts,tsx}",
    "infrastructure/**/*.{ts,tsx}",
    "registry/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}",
    "scripts/**/*.{ts,tsx,mjs}",
    "types/**/*.{ts,tsx}",
    "next.config.mjs",
    "tailwind.config.ts",
    "postcss.config.mjs",
    "vitest.config.ts",
    "next-i18next.config.ts",
    "components.json",
    "biome.jsonc",
    "tsconfig.json",
    "setupTests.ts"
  ],
  "project": [
    "app/**/*.{ts,tsx}",
    "widgets/**/*.{ts,tsx}",
    "features/**/*.{ts,tsx}",
    "shared/**/*.{ts,tsx}",
    "entities/**/*.{ts,tsx}",
    "compositions/**/*.{ts,tsx}",
    "infrastructure/**/*.{ts,tsx}",
    "registry/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}",
    "scripts/**/*.{ts,tsx,mjs}",
    "types/**/*.{ts,tsx}",
    "*.{ts,tsx,mjs,json,jsonc}"
  ],
  "ignore": [
    "shared/libs/data-grid/pages/**",
    "node_modules/**",
    ".next/**",
    ".git/**",
    "**/__tests__/**",
    "**/test/**",
    "**/*.test.{ts,tsx,js,jsx}",
    "**/*.spec.{ts,tsx,js,jsx}",
    "**/*.d.ts",
    "next-env.d.ts",
    "types/global.d.ts"
  ],
  "ignoreExportsUsedInFile": false,
  "ignoreDependencies": [
    "@biomejs/biome",
    "critters",
    "eslint",
    "eslint-config-next",
    "postcss",
    "postcss-load-config",
    "tailwindcss",
    "typescript",
    "@fortawesome/fontawesome-free",
    "@fortawesome/fontawesome-svg-core",
    "@fortawesome/free-solid-svg-icons",
    "@fortawesome/react-fontawesome",
    "@diceui/combobox",
    "color2k"
  ],
  "ignoreBinaries": ["python"]
}
