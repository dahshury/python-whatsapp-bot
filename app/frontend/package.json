  "scripts": {
    "preinstall": "node -e \"if(!(process.env.npm_config_user_agent||'').includes('pnpm')){console.error('Please use pnpm (corepack enable && pnpm).'); process.exit(1)}\"",
    "predev": "rm -rf .next",
    "dev": "next dev --turbo",
    "dev:classic": "next dev",
    "dev:clean": "rm -rf .next && next dev --turbo",
    "dev:reset": "rm -rf .next node_modules/.cache && pnpm install && next dev --turbo",
    "build": "next build",
    "build:classic": "next build",
    "start": "next start --turbo",
    "lint": "next lint",
    "lint:biome": "biome check .",
    "lint:fix": "biome check --fix --unsafe .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "knip": "knip",
    "knip:compact": "knip --reporter compact",
    "check": "pnpm typecheck && pnpm lint:biome && pnpm format:check",
    "fix": "pnpm lint:fix && pnpm format",
    "clean": "rm -rf .next out dist node_modules/.cache",
    "licenses:list": "pnpm licenses list --json > third_party_licenses.json",
    "licenses:notice": "pnpm licenses generate-disclaimer > THIRD_PARTY_NOTICES.txt",
    "test": "vitest run",
    "test:watch": "vitest"
  },
